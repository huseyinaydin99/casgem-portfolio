@model Casgem_Portfolio.Models.Entities.tbl_referance

@{
    ViewBag.Title = "ReferenceDetails";
    Layout = "~/Views/Shared/_PortfolioAdminLayout.cshtml";
}

<div class="card text-white bg-success mb-3" style="max-width: 20rem;">
    <div class="card-header">Referans</div>
    <div class="card-body">
        <h6 class="card-title">Referans Detayları Görüntüleniyor</h6>
        Telefon No: @Html.DisplayFor(x => x.PhoneNumber)
        <br />
        Referansın Adı: @Html.DisplayFor(x => x.NameSurname)
        <br />
        Mail Adresi: @Html.DisplayFor(x => x.Email)
    </div>
</div>
